LD_LIBRARY_PATH=@BGLBUILDLIBDIR@:$LD_LIBRARY_PATH
DYLD_LIBRARY_PATH=@BGLBUILDLIBDIR@:$DYLD_LIBRARY_PATH

export LD_LIBRARY_PATH
export DYLD_LIBRARY_PATH

if [ $1 = "-i" ]; then
  exec @BIGLOO@ "$@"
else  
  exec @BIGLOO@ @LDBIGLOOOPT@ "$@"
fi       
